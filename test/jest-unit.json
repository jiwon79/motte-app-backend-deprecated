{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "modulePaths": [
    "<rootDir>"
  ],
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/test/e2e/"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
